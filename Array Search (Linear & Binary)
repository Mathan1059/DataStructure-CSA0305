#include <stdio.h>

int linearSearch(int arr[], int n, int key) {
    for (int i = 0; i < n; i++)
        if (arr[i] == key) return i;
    return -1;
}

int binarySearch(int arr[], int n, int key) {
    int low = 0, high = n - 1;
    while (low <= high) {
        int mid = (low + high) / 2;
        if (arr[mid] == key) return mid;
        else if (arr[mid] < key) low = mid + 1;
        else high = mid - 1;
    }
    return -1;
}

int main() {
    int arr[6] = {10, 20, 30, 40, 50, 60};
    int key, pos;

    printf("Enter element to search: ");
    scanf("%d", &key);

    pos = linearSearch(arr, 6, key);
    if (pos != -1) printf("Linear Search: Found at index %d\n", pos);
    else printf("Linear Search: Not Found\n");

    pos = binarySearch(arr, 6, key);
    if (pos != -1) printf("Binary Search: Found at index %d\n", pos);
    else printf("Binary Search: Not Found\n");

    return 0;
}
